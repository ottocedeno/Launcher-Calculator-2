<table class="table table-sm table-bordered">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Property</th>
      <th scope="col">Value</th>
    </tr>
  </thead>
  <tbody>
    <% orbit.attributes.each do |key, value| %>
      <% unless ommitted_orbit_keys(key) %>
        <tr>
          <td><%= key.humanize %></td>
          <td><%= value %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>